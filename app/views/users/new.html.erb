<div class='rgs-form'>
	<h1>Register</h1>

	<%= form_for :user, url: users_path  do |f| %>
		<p>
			<%= f.label :username %><br>
			<%= f.text_field :username %>
		</p>

		<p>
			<%= f.label :password %><br>
			<%= f.text_field :password %>
		</p>

		<p>
			<%= f.submit %>	
		</p>	
	<% end %>
</div>


