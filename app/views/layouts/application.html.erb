<!DOCTYPE html>
<html>
<head>
  <title>RailsMapProject</title>

	<script src="http://maps.googleapis.com/maps/api/js?key=#{GOOGLE_API_KEY}"></script>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if current_user %>
		Signed in as <strong><%= current_user.email %></strong>
		<%= link_to "Sign out", signout_path, id: "sign_out" %>
	<% else %>
		<div><%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in" %></div>
		<div><%= link_to "Sign in with Facebook", "/auth/facebook", id: "sign_in" %></div>
	<% end %>

	<%= yield %>
</body>
</html>
